        <Label layoutX="100.0" layoutY="215.0" text="Name" />
        <Label layoutX="100.0" layoutY="255.0" text="Inv" />
        <Label layoutX="100.0" layoutY="295.0" text="Price/Cost" />
        <Label layoutX="100.0" layoutY="335.0" text="Max" />
        <Label layoutX="382.0" layoutY="335.0" text="Min" />
        <Label fx:id="addPartToggleLabel" layoutX="100.0" layoutY="375.0" text="Machine ID" />
        <Button fx:id="saveButton" layoutX="322.0" layoutY="471.0" mnemonicParsing="false" onAction="#savePart" text="Save" />
        <Button layoutX="404.0" layoutY="471.0" mnemonicParsing="false" onAction="#toMainScreen" text="Cancel" />
        <TextField fx:id="minTextField" layoutX="435.0" layoutY="331.0" />
        <TextField fx:id="machineIdTextField" layoutX="210.0" layoutY="371.0" />
    </children>
</AnchorPane>
