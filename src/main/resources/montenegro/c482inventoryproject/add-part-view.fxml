         </toggleGroup></RadioButton>
        <RadioButton fx:id="addOutsourcedRadioButton" layoutX="420.0" layoutY="73.0" mnemonicParsing="false" onAction="#onAddOutsourcedToggle" text="Outsourced" toggleGroup="$addPartToggleGroup" />
        <TextField disable="true" layoutX="180.0" layoutY="171.0" promptText="Auto Gen- Disabled">
            <font>
                <Font name="System Italic" size="13.0" />
            </font></TextField>
        <TextField fx:id="nameTextField" layoutX="180.0" layoutY="211.0" />
        <TextField fx:id="invTextField" layoutX="180.0" layoutY="251.0" />
        <TextField fx:id="priceTextField" layoutX="180.0" layoutY="291.0" />
        <TextField fx:id="maxTextField" layoutX="180.0" layoutY="331.0" />
