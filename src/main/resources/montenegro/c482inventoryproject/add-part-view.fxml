<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="621.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="montenegro.c482inventoryproject.addPartViewController">
    <children>
        <Label layoutX="74.0" layoutY="74.0" text="Add Part">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <RadioButton fx:id="addInhouseRadioButton" layoutX="306.0" layoutY="74.0" mnemonicParsing="false" onAction="#onAddInhouseToggle" selected="true" text="In-House">
